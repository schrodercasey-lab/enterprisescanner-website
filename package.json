{
  "name": "enterprise-scanner",
  "version": "2.1.0",
  "description": "Premium cybersecurity vulnerability assessment platform for Fortune 500 companies",
  "main": "backend/app.py",
  "scripts": {
    "start": "python backend/app.py",
    "dev": "python backend/app.py --debug",
    "build": "python deployment/scripts/build.py",
    "deploy": "python deployment/scripts/deploy.py",
    "test": "python -m pytest backend/tests/",
    "lint": "flake8 backend/",
    "security-scan": "bandit -r backend/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enterprise-scanner/platform"
  },
  "keywords": [
    "cybersecurity",
    "vulnerability-assessment",
    "fortune-500",
    "enterprise-security",
    "compliance"
  ],
  "author": "Enterprise Scanner Team",
  "license": "Proprietary",
  "dependencies": {
    "flask": "^2.3.3",
    "flask-cors": "^4.0.0",
    "flask-sqlalchemy": "^3.0.5",
    "psycopg2-binary": "^2.9.7",
    "celery": "^5.3.1",
    "redis": "^4.6.0",
    "requests": "^2.31.0",
    "cryptography": "^41.0.3",
    "jwt": "^1.3.1",
    "boto3": "^1.28.25"
  },
  "devDependencies": {
    "pytest": "^7.4.0",
    "flake8": "^6.0.0",
    "bandit": "^1.7.5",
